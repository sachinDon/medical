<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android" android:layout_width="match_parent"
    android:layout_height="match_parent">

    <RelativeLayout
        android:id="@+id/cutom_dilaog_address"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginEnd="20dp"
        android:background="@drawable/round_lightgray"
        >


        <TextView
            android:id="@+id/textview_address_divtitle"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textSize="18dp"
            android:gravity="center"
            android:fontFamily="@font/sanfranciscodisplay_semibold"
            android:layout_marginTop="10dp"
            android:layout_marginBottom="10dp"
            android:textColor="@color/colorblack"
            android:text="Select a delivery address"
            />

<RelativeLayout
    android:id="@+id/relative_add1"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginTop="1dp"
    android:layout_below="@+id/textview_address_divtitle"
    android:background="@drawable/blackline_border">

    <TextView
        android:id="@+id/textview_address_title11"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="17dp"
        android:fontFamily="@font/sanfranciscodisplay_medium"
        android:layout_marginTop="10dp"
        android:layout_marginBottom="5dp"
        android:layout_marginStart="15dp"
        android:textColor="@color/colorblack"
        android:text="Address1:"
        />
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textview_address_title11">

        <TextView
            android:id="@+id/text_select_add_div"
            android:layout_width="28dp"
            android:layout_height="28dp"
            android:layout_marginStart="15dp"
            android:layout_centerVertical="true"
            android:background="@drawable/checkadd"/>

        <TextView
            android:id="@+id/text_add_div_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="15dp"
            android:fontFamily="@font/sanfranciscodisplay_medium"
            android:textColor="@color/colorblack"
            android:textSize="15sp"
            android:layout_marginEnd="15dp"
            android:layout_toEndOf="@+id/text_select_add_div"
            android:text="Sachin ramling mokashi"/>

        <TextView
            android:id="@+id/text_add_div_addres1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="15dp"
            android:layout_marginEnd="15dp"
            android:layout_marginBottom="15dp"
            android:fontFamily="@font/sanfranciscodisplay_medium"
            android:textColor="@color/colorblack"
            android:textSize="15sp"
            android:layout_below="@+id/text_add_div_name"
            android:layout_toEndOf="@+id/text_select_add_div"
            android:text="A/p neharungar nimani tal tasgoan dist sangli"/>



    </RelativeLayout>



</RelativeLayout>

        <RelativeLayout
            android:id="@+id/relative_address2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="4dp"
            android:layout_below="@+id/relative_add1"
            android:background="@drawable/blackline_border">

            <TextView
                android:id="@+id/textview_address_title22"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textSize="17dp"
                android:fontFamily="@font/sanfranciscodisplay_medium"
                android:layout_marginTop="10dp"
                android:layout_marginBottom="5dp"
                android:layout_marginStart="15dp"
                android:textColor="@color/colorblack"
                android:text="Address2:"
                />


            <RelativeLayout
                android:id="@+id/relative_address2_addm"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/textview_address_title22">

                <TextView
                    android:id="@+id/text_address2_addaddress"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Add Address"
                    android:textSize="16sp"
                    android:fontFamily="@font/sanfranciscodisplay_semibold"
                    android:textColor="@color/colortab"
                    android:layout_centerHorizontal="true"
                    android:layout_centerVertical="true"
                    android:paddingTop="10dp"
                    android:paddingBottom="10dp"
                    android:paddingLeft="8dp"
                    android:paddingRight="8dp"
                    android:layout_marginTop="10dp"
                    android:layout_marginBottom="15dp"
                    android:background="@drawable/border_color_logo"/>

            </RelativeLayout>


            <RelativeLayout
                android:id="@+id/relative_address_address2_text"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:visibility="visible"
                android:layout_below="@+id/textview_address_title22">

                <TextView
                    android:id="@+id/text_select_add_div2"
                    android:layout_width="28dp"
                    android:layout_height="28dp"
                    android:layout_marginStart="15dp"
                    android:layout_centerVertical="true"
                    android:background="@drawable/uncheckadd"/>

                <TextView
                    android:id="@+id/text_add_div_name2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="15dp"
                    android:fontFamily="@font/sanfranciscodisplay_medium"
                    android:textColor="@color/colorblack"
                    android:textSize="15sp"
                    android:layout_marginEnd="15dp"
                    android:layout_toEndOf="@+id/text_select_add_div2"
                    android:text="Sachin ramling mokashi"/>

                <TextView
                    android:id="@+id/text_add_div_addres12"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="15dp"
                    android:layout_marginEnd="15dp"
                    android:layout_marginBottom="15dp"
                    android:fontFamily="@font/sanfranciscodisplay_medium"
                    android:textColor="@color/colorblack"
                    android:textSize="15sp"
                    android:layout_below="@+id/text_add_div_name2"
                    android:layout_toEndOf="@+id/text_select_add_div2"
                    android:text="A/p neharungar nimani tal tasgoan dist sangli"/>



            </RelativeLayout>




        </RelativeLayout>

        <TextView
            android:id="@+id/text_editdeliverytoadd"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Edit address"
            android:background="@drawable/round_white1"
            android:layout_below="@+id/relative_address2"
            android:layout_marginTop="15dp"
            android:layout_marginStart="20dp"
            android:layout_marginEnd="20dp"
            android:paddingTop="10dp"
            android:paddingBottom="10dp"
            android:gravity="center"
            android:textColor="@color/colortab"
            android:textSize="16sp"
            android:fontFamily="@font/sanfranciscodisplay_semibold"
            android:layout_centerHorizontal="true"/>

        <TextView
            android:id="@+id/text_deliverytoadd"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Delivery to this address"
            android:background="@drawable/round_golden"
            android:layout_below="@+id/text_editdeliverytoadd"
            android:layout_marginTop="15dp"
            android:layout_marginStart="20dp"
            android:layout_marginEnd="20dp"
            android:paddingTop="10dp"
            android:paddingBottom="10dp"
            android:gravity="center"
            android:textSize="16sp"
            android:textColor="@color/colorblack"
            android:fontFamily="@font/sanfranciscodisplay_semibold"
            android:layout_centerHorizontal="true"/>

        <TextView
            android:id="@+id/text_canceldeliverytoadd"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Cancel"
            android:background="@drawable/round_white1"
            android:layout_below="@+id/text_deliverytoadd"
            android:layout_marginTop="10dp"
            android:layout_marginStart="20dp"
            android:layout_marginEnd="20dp"
            android:paddingTop="10dp"
            android:paddingBottom="10dp"
            android:layout_marginBottom="20dp"
            android:gravity="center"
            android:textColor="@color/colorred"
            android:textSize="16sp"
            android:fontFamily="@font/sanfranciscodisplay_semibold"
            android:layout_centerHorizontal="true"/>

    </RelativeLayout>



</RelativeLayout>